{
  "name": "rdo-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "migrate": "tsx src/scripts/run_migration.ts",
    "test": "vitest",
    "prepare": "husky",
    "check-size": "size-limit",
    "analyze": "size-limit --why"
  },
  "size-limit": [
    {
      "path": "dist/assets/*.js",
      "limit": "90 kB",
      "name": "Main JS Bundle"
    },
    {
      "path": "dist/assets/*.css",
      "limit": "10 kB",
      "name": "Styles"
    }
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@size-limit/file": "^12.0.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "size-limit": "^12.0.0",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.21.0",
    "vite": "^7.2.4",
    "vitest": "^2.1.9"
  }
}
